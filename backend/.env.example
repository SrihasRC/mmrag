# Environment Variables Template for Multimodal RAG API
# Copy this file to .env and fill in your actual API keys

# Cohere API Key (Required for embeddings)
COHERE_API_KEY=your_cohere_api_key_here

# Google API Key (Required for image summarization and QA with Gemini 2.5 Flash)
GOOGLE_API_KEY=your_google_api_key_here

# Groq API Key (Required for text/table summarization)
GROQ_API_KEY=your_groq_api_key_here

# LangChain Configuration (Optional)
LANGCHAIN_API_KEY=your_langchain_api_key_here
LANGCHAIN_TRACING_V2=true

# Application Configuration
APP_ENV=development
LOG_LEVEL=INFO

# Storage Configuration
STORAGE_PATH=./storage
VECTOR_DB_PATH=./storage/vector_db
CONTENT_PATH=./storage/content
UPLOADS_PATH=./storage/uploads
